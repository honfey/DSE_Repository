@model IEnumerable<SIS.Models.CourseWork>

@{
    ViewBag.Title = "Index";
}

<h2>CourseWork</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
@using (Html.BeginForm())
{
    <p>
        Find By TestType: @Html.TextBox("search")
        <input type="submit" value="Search" />
    </p>
}
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model=>model.ClassStudent.Course_ModuleId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ClassStudent.Student.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TestType.Name)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <th>
                @Html.DisplayFor(modelItem => item.ClassStudent.Course_ModuleId)
            </th>
            <td>
                @Html.DisplayFor(modelItem => item.ClassStudent.Student.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TestType.Name)
            </td>
            <td>
                @*@Html.ActionLink("MultipleEdit", "EditList", new { id = item.ClassStudent.Course_ModuleId })*@
                @Html.ActionLink("Details", "Details", new { id = item.Id })
            </td>
        </tr>
    }

</table>
@Html.ActionLink("MultipleEdit", "EditList")
